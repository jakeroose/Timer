<%= form_for time_frame do |f| %>
  <ul>
    <% time_frame.errors.full_messages.each do |e| %>
      <li><%= e %></li>
    <% end %>
  </ul>
  <div class='field form-group'>
    <%= f.label :time_elapsed %><br>
    <%= f.text_field :time_elapsed %>
  </div>

  <div class='field form-group'>
    <%= f.label :description %><br>
    <%= f.text_field :description %>
  </div>

  <div class="field form-group">
    <%= f.check_box :active %>
    <%= f.label :active %>
  </div>

  <p>
    <%= f.submit %>
  </p>
<% end %>
